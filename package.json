{
  "name": "root",
  "version": "0.0.0",
  "packageManager": "pnpm@10.9.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run --filter=./packages/examples -r dev",
    "build": "pnpm run build:ui && pnpm run build:examples",
    "build:examples": "pnpm run --filter=./packages/examples -r build",
    "build:ui": "pnpm run --filter=./packages/ui -r build",
    "lint": "pnpm --recursive run lint",
    "test": "pnpm run test:ui",
    "test:ui": "pnpm run --filter=./packages/ui -r test",
    "publish:ui": "pnpm run test:ui && pnpm run build:ui && pnpm publish --filter=./packages/ui --access public --no-git-checks"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "prettier": "3.5.3",
    "sass-embedded": "^1.87.0",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.1.1",
    "vue-tsc": "^2.2.8"
  }
}
